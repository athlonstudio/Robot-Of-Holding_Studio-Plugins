{
  "manifest_version": 3,
  "name": "Robot of Holding",
  "version": "0.3.0",
  "description": "Save externally opened links into a clickup board.",
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "images/x16.png",
    "32": "images/x32.png",
    "48": "images/x48.png",
    "128": "images/x128.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
      "index.html",
      "./scripts/requests.js",
      "./scripts/plugin-logic.js",
      "./styles/index.html",
      "./styles/popup.css"
   ],
  "content_scripts": [ {
      "js": ["/index.js"],
      "matches": ["<all_urls>"],
      "match_about_blank": false
    } ]
}
